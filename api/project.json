                "Nancy.Authentication.Basic": "2.0.0-clinteastwood",
                "Npgsql.EntityFrameworkCore.PostgreSQL": "1.0.0",
                "Dapper": "1.50.2",
                //"Microsoft.EntityFrameworkCore.Design": "1.0.0-preview2-final"
  
                /*"commands": {
                    "web": "Microsoft.AspNet.Server.Kestrel"
